<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.wzh.tmall.dao.ProductDao">

    <resultMap type="com.wzh.tmall.entity.Product" id="ProductMap">
        <result property="id" column="id" jdbcType="INTEGER"/>
        <result property="name" column="name" jdbcType="VARCHAR"/>
        <result property="subtitle" column="subTitle" jdbcType="VARCHAR"/>
        <result property="originalprice" column="originalPrice" jdbcType="VARCHAR"/>
        <result property="promoteprice" column="promotePrice" jdbcType="VARCHAR"/>
        <result property="stock" column="stock" jdbcType="INTEGER"/>
        <result property="cid" column="cid" jdbcType="INTEGER"/>
        <result property="createdate" column="createDate" jdbcType="TIMESTAMP"/>
    </resultMap>


</mapper>

